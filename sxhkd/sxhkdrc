super + Return
          rofi -show drun

super + {1-5}
	   bspc desktop -f '^{1-5}'

super + shift + {1-5}
           bspc node -d '^{1-5}'

super + Left
	   bspc node -f prev.local.window

super + Right
	   bspc node -f next.local.window
           
super + shift + {Left,Down,Up,Right}
           bspc node -p {west, south, north, east}

super + q
           bspc node -c

super + k
	   bspc node -k

super + f
           gnome-screenshot

super + @space
		   bspc node -t ~floating
